{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Title": {
      "type": "string",
      "maxLength": 250,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "MetaTag": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta-tag"
    },
    "OpenGraph": {
      "type": "component",
      "repeatable": false,
      "component": "seo.open-graph"
    },
    "StructuredData": {
      "type": "component",
      "repeatable": true,
      "component": "seo.structure-data"
    },
    "Layout": {
      "type": "dynamiczone",
      "components": [
        "content.rte"
      ]
    }
  }
}
