{
  "collectionName": "components_form_forms",
  "info": {
    "displayName": "Form",
    "icon": "dashboard"
  },
  "options": {},
  "attributes": {
    "Inputs": {
      "type": "component",
      "component": "form.input",
      "repeatable": true
    },
    "Setting": {
      "type": "component",
      "component": "setting.color",
      "repeatable": false
    },
    "Submit": {
      "type": "string"
    },
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "DocumentText": {
      "type": "string"
    },
    "DocumentLogos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "ResolveHeading": {
      "type": "component",
      "component": "content.heading",
      "repeatable": false
    },
    "ResolveContent": {
      "type": "customField",
      "customField": "plugin::tinymce.tinymce"
    }
  },
  "config": {}
}
