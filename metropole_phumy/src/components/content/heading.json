{
  "collectionName": "components_content_headings",
  "info": {
    "displayName": "Heading",
    "description": ""
  },
  "options": {},
  "attributes": {
    "Tag": {
      "type": "enumeration",
      "enum": [
        "h1",
        "h2",
        "h3",
        "h4",
        "h5",
        "h6",
        "p"
      ],
      "default": "h2",
      "required": true
    },
    "Color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "Transform": {
      "type": "enumeration",
      "enum": [
        "Capitalize",
        "lowercase",
        "UPPERCASE",
        "none"
      ],
      "default": "UPPERCASE"
    },
    "Align": {
      "type": "enumeration",
      "enum": [
        "Left",
        "Right",
        "Center"
      ]
    },
    "Text": {
      "type": "string"
    }
  }
}
